{
  "name": "electron-midi",
  "version": "0.0.1-alpha1",
  "scripts": {
    "preinstall": "npm install node-pre-gyp",
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "node test/virtual-loopback-test-automated.js"
  },
  "main": "midi.js",
  "description": "MIDI hardware IO",
  "author": {
    "name": "Ahad Cove",
    "email": "me@ahadcove.com",
    "url": "http://www.ahadcove.com/"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.8.0"
  },
  "dependencies": {
    "bindings": "~1.5.0",
    "nan": "^2.3.3",
    "node-pre-gyp": "^0.6.39"
  },
  "binary": {
    "module_name": "midi",
    "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://github.com/ahadcove/electron-midi/releases/download/",
    "remote_path": "{version}"
  },
  "devDependencies": {
    "mocha": "^10.2.0",
    "should": ">= 5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahadcove/electron-midi.git"
  }
}
